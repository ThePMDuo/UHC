parameters:
    level: 7

    checkMissingIterableValueType: false
    reportUnmatchedIgnoredErrors: true

    ignoreErrors:
        - 'Call to method setHealthPercent'
        - 'Call to method setTitle'
        - 'Return typehint of method AGTHARN'
        - 'Instantiated class'

    paths:
        - /source/src
    bootstrapFiles:
        - phar:///pocketmine/PocketMine-MP.phar/vendor/autoload.php
    scanDirectories:
        - /source/src
    excludes_analyse:
        - source/vendor
